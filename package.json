{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "dev_docker": "cross-env NODE_ENV=development vite --port 8080 --host 0.0.0.0",
    "build": "cross-env NODE_ENV=production run-p type-check \"build-only {@}\" --",
    "release": "cross-env APP_RELEASE=true run-p build",
    "preview": "vite preview",
    "preview_docker": "vite preview --port 8080 --host 0.0.0.0",
    "preview_release": "vite preview --outDir \"release\"",
    "preview_docker_release": "vite preview --outDir \"release\" --port 8080 --host 0.0.0.0",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.2.0",
    "vue": "^3.4.35",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.2",
    "vue3-lazyload": "^0.3.8"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.14",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.1",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "svgo": "^3.3.2",
    "typescript": "~5.4.5",
    "vite": "^5.3.5",
    "vite-plugin-compression2": "^1.1.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.29"
  }
}
